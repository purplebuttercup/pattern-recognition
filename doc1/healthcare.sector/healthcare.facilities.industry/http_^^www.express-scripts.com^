<SCRIPT LANGUAGE="JavaScript">

function MakeArray(n)

    {
        this.length = n
        return this
    }

months = new MakeArray(12)
months[1] = "January"
months[2] = "February"
months[3] = "March"
months[4] = "April"
months[5] = "May"
months[6] = "June"
months[7] = "July"
months[8] = "August"
months[9] = "September"
months[10] = "October"
months[11] = "November"
months[12] = "December"

days = new MakeArray(7)
days[1] = "Sunday"
days[2] = "Monday"
days[3] = "Tuesday"
days[4] = "Wednesday"
days[5] = "Thursday"
days[6] = "Friday"
days[7] = "Saturday"

function today(getDate)
    
    {
       var Day = days[getDate.getDay() + 1]
       var Month = months[getDate.getMonth() + 1]
       var Year = getDate.getYear() + 1900

       return Day + ", " + Month + " " + getDate.getDate() + ", " + Year
    }

function news()

    {
       ms = 24*60*60*1000
       var Expire = new Date("November 10, 1997"),
           Now    = new Date(),
           left   = Math.round((Expire.getTime() - Now.getTime())/ms);
       
          if (left > 0)
             {document.write ("<IMG SRC='/esi/images/new.gif' HEIGHT=16 WIDTH=16 BORDER=0>")}
          else 
             {}
     }                 


</SCRIPT>

