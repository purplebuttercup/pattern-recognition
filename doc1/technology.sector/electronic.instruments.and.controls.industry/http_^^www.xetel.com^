language="JavaScript"><!--
//Hide JavaScript from Java-Impaired Browsers

var browser = 0;

if (navigator.userAgent.indexOf("Mozilla/3.0") != -1)
	browser = 1;

if (browser == 1) {
	var buttPic = new Array();

	buttPic[0] = new Image(126,160);
	buttPic[0].src = "images/tab1.gif";
	buttPic[1] = new Image(126,160);
	buttPic[1].src = "images/tab10.gif";


	buttPic[2] = new Image(126,160);
	buttPic[2].src = "images/tab2.gif";
	buttPic[3] = new Image(126,160);
	buttPic[3].src = "images/tab20.gif";

	buttPic[4] = new Image(126,160);
	buttPic[4].src = "images/tab3.gif";
	buttPic[5] = new Image(126,160);
	buttPic[5].src = "images/tab30.gif";

	buttPic[6] = new Image(126,160);
	buttPic[6].src = "images/tab4.gif";
	buttPic[7] = new Image(126,160);
	buttPic[7].src = "images/tab40.gif";

}



function isnscroll(hype) {

 var shameless = 100

 var prtspc = " ";

 var col = 1;

 /* You could use array here, but this is easy way to load messages
    Unlike HTML documents, multiple adjoining spaces are preserved */
 var promotional=" Manufacturing the Future Today..."
+" XeTel brings you Micro BGA and Miniature Card Assembly Services-  Contact us at 1.888.GO.XETEL                ";





 if (hype>shameless) {
  hype--;
  var cmd="isnscroll(" + hype + ")";
  isntimer=window.setTimeout(cmd,shameless);
 }

 else if (hype<=shameless && hype>0) {
  for (col=0;col<hype;col++) {
   prtspc+=" ";
  }

  prtspc+=promotional;
  hype--;
  var cmd="isnscroll(" + hype + ")";
  window.status=prtspc;
  isntimer=window.setTimeout(cmd,shameless);
 }

 else if (hype<=0) {
  if (-hype<promotional.length) {
   prtspc+=promotional.substring(-hype,promotional.length);
   hype--;
   var cmd="isnscroll(" + hype + ")";
   window.status=prtspc;
   isntimer=window.setTimeout(cmd,100);
  }

  else {
   window.status=" ";
   isntimer=window.setTimeout("isnscroll(100)",75);
  }
 }
}

function startit() {

 isntimer=window.setTimeout("isnscroll(100)",500);

}



function LiteButton(which, mouseOver)
{
	if (browser == 1)
	{
		if (which == 0) {
			document.first.src = buttPic[2 * which + mouseOver].src;
			//window.status = mouseOver ? "Company Information" : "";
		   document.about.T1.value = "[Executive management/Investor Relations/Press Release/Employment Oportunities]";
         return true;
		} else if (which == 1) {
			document.second.src = buttPic[2 * which + mouseOver].src;
			//window.status = mouseOver ? "Technology" : "";
			document.about.T1.value = "[BGA/Scale Package/Technology PR]";
		return true;
		} else if (which == 2) {
			document.third.src = buttPic[2 * which + mouseOver].src;
			//window.status = mouseOver ? "Contract Manufacturing Services" : "";
		   document.about.T1.value = "[Manufacturing Services/EDI Capabilities/Package Assembly/Equipment List/Sales & Marketing]";
		return true;
		} else if (which == 3) {
			document.fourth.src = buttPic[2 * which + mouseOver].src;
			//window.status = mouseOver ? "Facility Facts" : "";
		  document.about.T1.value = "Facility Facts for [Austin/Dallas/San Ramon]";
   	return true;
     }
	}
	
	return false;
}
// Stop Hiding Contents 

// --></script>

