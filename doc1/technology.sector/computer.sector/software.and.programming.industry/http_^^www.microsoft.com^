function turnRed() {
	what = window.event.srcElement;
	if (what.tagName == "IMG") {
		what.src = what.src.substring(0,(what.src.indexOf(".gif"))) + "-red.gif";
		window.event.cancelBubble = true;
	}
}

function turnWhite () {
	what = window.event.srcElement
	if (what.tagName == "IMG") {
		what.src = what.src.substring(0,(what.src.indexOf("-red.gif"))) + ".gif";
		window.event.cancelBubble = true;
	}
}
//-->
