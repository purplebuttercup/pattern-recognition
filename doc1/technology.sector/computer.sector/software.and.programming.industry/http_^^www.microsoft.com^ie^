<SCRIPT LANGUAGE="JavaScript"><!--
var ua = navigator.userAgent
var ie4 = ua.indexOf("MSIE 4.0");
var win31PP1 = ua.indexOf("MSIE 4.0;Windows 3.1"); //this is to deal with the non-standard UA string
var betaVer = 0
var beta = false
if (win31PP1 >=1)
	beta = true;

if (ie4 != -1) {
	if ((ua.charAt(ie4+8) == "b") || (ua.charAt(ie4+8) == "p")) {
		beta = true;
		betaVer = ua.charAt(ie4+9); //returns beta version number
	}
}

var ie4Release = ((ie4 != -1) && (!beta) && ((ie4 != -1) && (win31PP1 == -1)));
//--></SCRIPT>

<LINK ID=ie3ss REL=STYLESHEET TYPE="text/css" HREF="/ie/global/ie3left.css">

<SCRIPT LANGUAGE="JavaScript"><!--
var apV = navigator.appVersion;
var mac = apV.indexOf('Macintosh') != -1;
var IE = (apV.indexOf('MSIE') != -1) && !mac;
var isIE3 = IE && (apV.indexOf('MSIE 3') != -1);
var isIE4 = IE && parseInt(apV) == 4;
var NN3 = navigator.appName == 'Netscape' && parseInt(apV) == 3 && !mac;
var NN4 = navigator.appName == 'Netscape' && parseInt(apV) == 4 && !mac;

if (!NN3 && !isIE3){
	document.writeln('<LINK REL=STYLESHEET TYPE="text/css" HREF=');
	if (isIE4){
		ie3ss.disabled = true;
		document.writeln('"/ie/global/ie4left.css">');
	}
	else if (NN4)
		document.writeln('"/ie/global/nav4left.css">');
	else if (mac)
		document.writeln('"/ie/global/macleft.css">');
}
//--></SCRIPT>

<SCRIPT LANGUAGE="javascript"><!--
function turnRed() {
	what = window.event.srcElement;
	if (what.tagName == "IMG") {
		what.src = what.src.substring(0,(what.src.indexOf(".gif"))) + "-red.gif";
		window.event.cancelBubble = true;
	}
}

function turnWhite () {
	what = window.event.srcElement
	if (what.tagName == "IMG") {
		what.src = what.src.substring(0,(what.src.indexOf("-red.gif"))) + ".gif";
		window.event.cancelBubble = true;
	}
}
//--></SCRIPT>

