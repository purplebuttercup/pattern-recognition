<!-- Hiding script from old browsers

bName = navigator.appName;
bVer = parseInt(navigator.appVersion);

if      (bName == "Netscape" && bVer >= 3) ver = "n3";
else if (bName == "Netscape" && bVer == 2) ver = "n2"; else if (bName == "Microsoft Internet Explorer" && bVer >= 2) ver = "e3";

if (ver == "n3") {
button0=new Image();
button1=new Image();
button10=new Image();
button11=new Image();
button12=new Image();
button20=new Image();
button21=new Image();
button22=new Image();
button30=new Image();
button31=new Image();
button32=new Image();
button40=new Image();
button41=new Image();
button42=new Image();
button50=new Image();
button51=new Image();
button52=new Image();
button60=new Image();
button61=new Image();
button62=new Image();



button0.src="/images/no_desc.gif";
button1.src="/images/descr_link3.gif";
button10.src="/images/news_blue.gif";
button11.src="/images/news_red.gif";
button12.src="/images/news_desc.gif";
button20.src="/images/press_blue.gif";
button21.src="/images/press_red.gif";
button22.src="/images/press_desc.gif";
button30.src="/images/ops_blue.gif";
button31.src="/images/ops_red.gif";
button32.src="/images/ops_desc.gif";
button40.src="/images/emp_blue.gif";
button41.src="/images/emp_red.gif";
button42.src="/images/emp_desc.gif";
button50.src="/images/invest_blue.gif";
button51.src="/images/invest_red.gif";
button52.src="/images/invest_desc2.gif";
button60.src="/images/trade_blue.gif";
button61.src="/images/trade_red.gif";
button62.src="/images/descr_link3.gif";



}

timer = 0;

function display(num) {
	if (ver == "n3")   {
		clearTimeout(timer);
		if (num=="0") document.description.src=button0.src;
		if (num=="10") document.news.src=button10.src;
		if (num=="11") document.news.src=button11.src;
		if (num=="12") document.description.src=button12.src;
		if (num=="20") document.press.src=button20.src;
		if (num=="21") document.press.src=button21.src;
		if (num=="22") document.description.src=button22.src;
		if (num=="30") document.ops.src=button30.src;
		if (num=="31") document.ops.src=button31.src;
		if (num=="32") document.description.src=button32.src;
		if (num=="40") document.emp.src=button40.src;
		if (num=="41") document.emp.src=button41.src;
		if (num=="42") document.description.src=button42.src;
		if (num=="50") document.invest.src=button50.src;
		if (num=="51") document.invest.src=button51.src;
		if (num=="52") document.description.src=button52.src;
		if (num=="60") document.trade.src=button60.src;
		if (num=="61") document.trade.src=button61.src;
		if (num=="62") document.description.src=button62.src;
	}
}

function changeback() {
	if ( ver == "n3" ) {
		document.description.src = button1.src;
	}
}
//-->
