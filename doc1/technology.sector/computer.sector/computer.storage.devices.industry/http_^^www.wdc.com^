  function MakeImageArray(n) 
  {
        this.length = n
        for (var i = 0; i<=n; i++) {
             this[i] = new Image()
                                   }
        return this
  }

  function MakeStringArray(n) 
  {
        this.length = n
        for (var i = 0; i<=n; i++) {
             this[i] = new String()
                                   }
        return this
  }

  function msover(num) 
  {
        if ( browser )
          {
             document.images[num].src = over[num].src;
             document.images[5].src = image[num].src
           }
  }

  function msout(num) 
  {
        if ( browser )
          { 
	     document.images[num].src = other[num].src
             document.images[5].src = image[10].src
           }
  }

  function msanim()    
  {
   	 if ( browser )
	   {
	     document.images[5].src = image[5].src
  	   } 
  }

