}

function animateup(){
        
        counter = (counter < 7) ? (counter + 1) : 1
        document.anim.src = imgs[counter].src
        if (counter == 7)
        	timer=setTimeout("animatedown()", 50)
        else 
        	timer=setTimeout("animateup()", 50)
        	
        
}

function animatedown(){
        
        counter = (counter > 1) ? (counter - 1) : 1
        document.anim.src = imgs[counter].src
        if (counter == 1) {
        	if (sentry == 2) {
        		stop()
        		}
        	else {
        		sentry++ 
        		timer=setTimeout("animateup()", 50)
        		}
        	}
        else 
        	timer=setTimeout("animatedown()", 50)
        	
        
}

