{       if (version == "n3") 
{               imgOn = eval(imgName + "on.src");        
document [imgName].src = imgOn; }}function img_inact(imgName)
{      if (version == "n3") 
{        imgOff = eval(imgName + "off.src");        document [imgName].src = imgOff;    }}

