<!--  Start of comments

function handleClick(theButton, theForm)
{
//   theForm.who.value = theButton.name;
   if (theButton.name == "or_all")
   {  
        theForm.or_press.checked = 0;
        theForm.or_financial.checked = 0;
        theForm.or_location.checked = 0;
        theForm.or_history.checked = 0;
        theForm.or_groups.checked = 0;
        theForm.or_divisions.checked = 0;
        theForm.or_products.checked = 0;
        theForm.or_markets.checked = 0;
   }
   else
   {
        theForm.or_all.checked = 0;
   }
}

// End of Script -->
</SCRIPT>

