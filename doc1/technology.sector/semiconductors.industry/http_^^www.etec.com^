      function MakeArray(n) {
              this.length = n
              for (var i = 1; i<=n; i++) {
                      this[i] = new Image()
                      }
              return this
      }

        function msover(num) {
                        if ( browser) { 
                                document.images[num+1].src = over[num+1].src
                                window.status = blurb[num+1]
                                document.images[0].src = picture[num+1].src
                        }
                }

        function msout(num) {
                        if ( browser) {
                                        document.images[num+1].src = normal[num+1].src
                                        document.images[0].src = picture[8].src
                                        window.status = blurb[8]
                        }                   
                }


