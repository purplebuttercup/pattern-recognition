<!-- The following JavaScript will only work in Netscape 3.0 or higher -->
<!-- or Internet Explorer 4.0 or higher.                               -->
<!--                                                                   -->
<!-- To change the swaping images, make the change in the var          -->
<!-- statements immediately below.  Be certain to update the location  -->
<!-- of the images by changing the baseURL variable.                   -->

<SCRIPT LANGUAGE="JavaScript1.1">
<!--//

// The baseURL set the global location of the GIF images.
var baseURL = ""

// Set up compatiblity of JavaScript to Browser.

var compat = "false"


// The following statments verify the browser make and version.
// Note, all JavaScript from here on must contain the IF statement
// for compatiblity.

browserName = navigator.appName;

browserVer = parseInt(navigator.appVersion);

if (browserName == "Netscape" && browserVer >= 3) compat = "true";

else if (browserName == "Microsoft Internet Explorer" && browserVer >= 4) compat = "true";


// Pre-loading the rollovers.

if (compat == 'true') {

	var imgOriginal = new Image()
	imgOriginal.src = baseURL+"/gifs/right.gif"
	
	var imgSpecial = new Image()
	imgSpecial.src = baseURL+"/gifs/specialroll.gif"

	var imgStory = new Image()
	imgStory.src = baseURL+"/gifs/storyroll.gif"

	var imgHongKong = new Image()
	imgHongKong.src = baseURL+"/gifs/bleft4-roll.jpg"

	var imgSpareChange = new Image()
	imgSpareChange.src = baseURL+"/gifs/spareroll.gif"

	var imgBusiness = new Image()
	imgBusiness.src = baseURL+"/gifs/bizroll.gif"

	var imgPersonal = new Image()
	imgPersonal.src = baseURL+"/gifs/personalroll.gif"

	var imgEconomics = new Image()
	imgEconomics.src = baseURL+"/gifs/econroll.gif"

	var imgCorporate = new Image()
	imgCorporate.src = baseURL+"/gifs/corporateroll.gif"

	var imgBAToday = new Image()
	imgBAToday.src = baseURL+"/gifs/batodayroll.gif"
	
	var imgByob = new Image()
	imgByob.src = baseURL+"/gifs/byobroll.gif"

}


// Defining the rollovers.

function LoadSpecial() {
if (compat == 'true') {
	showDefault = false;
	document.images["rollover"].src = imgSpecial.src;
	}
	}
	
function LoadStory() {
if (compat == 'true') {
	showDefault = false;
	document.images["rollover"].src = imgStory.src;
	}
	}

function LoadHongKong() {
if (compat == 'true') {
	showDefault = false;
	document.images["rollover"].src = imgHongKong.src;
	}
	}
	
function LoadSpareChange() {
if (compat == 'true') {
	showDefault = false;
	document.images["rollover"].src = imgSpareChange.src;
	}
	}
	
function LoadBusiness() {
if (compat == 'true') {
	showDefault = false;
	document.images["rollover"].src = imgBusiness.src;
	}
	}
	
function LoadPersonal() {
if (compat == 'true') {
	showDefault = false;
	document.images["rollover"].src = imgPersonal.src;
	}
	}
	
function LoadEconomics() {
if (compat == 'true') {
	showDefault = false;
	document.images["rollover"].src = imgEconomics.src;
	}
	}

function LoadCorporate() {
if (compat == 'true') {
	showDefault = false;
	document.images["rollover"].src = imgCorporate.src;
	}
	}

function LoadBAToday() {
if (compat == 'true') {
	showDefault = false;
	document.images["rollover"].src = imgBAToday.src;
	}
	}
	
function LoadByob() {
if (compat == 'true') {
    showDefault = false;
	document.images["rollover"].src = imgByob.src;
	}
	}

function setDefault() {
	if (showDefault) {
		document.images["rollover"].src = imgOriginal.src;
	}
	}

function unLoad() {
if (compat == 'true') {
        showDefault = true;
		setTimeout('setDefault();', 1);
	}
}
	
//-->


