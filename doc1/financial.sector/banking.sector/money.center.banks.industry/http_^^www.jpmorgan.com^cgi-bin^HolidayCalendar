        <FONT SIZE="3" COLOR="#006666"><b>Holidays on a particular day</b></FONT><br>
        Select a year and month and enter a date.
	<FORM ACTION="/cgi-bin/HolidayCalendar98" METHOD="POST">
	<SELECT NAME="yr">
	<OPTION VALUE="01" SELECTED>1998
	<OPTION VALUE="02">1997
	<OPTION VALUE="03">1996</SELECT> 
	<SELECT NAME="month">
	<OPTION VALUE="01">January
	<OPTION VALUE="02">February
	<OPTION VALUE="03">March
	<OPTION VALUE="04">April
	<OPTION VALUE="05">May
	<OPTION VALUE="06">June
