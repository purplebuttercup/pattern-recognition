 function img_act(imgName)
 {  if (version=="good")
    {   imgOn = eval(imgName + "_s.src");
        document[imgName].src = imgOn;
        descrimgOn = eval(imgName + "_d.src");
        document["desc"].src = descrimgOn;
    }
 }
 function img_inact(imgName)
 {  if (version=="good")
    {   document[imgName].src = eval(imgName + "_f.src");
        document["desc"].src = "graphics/story.gif";
    }
 }
 function mast_act(imgName)
 {  if (version=="good")
    {   imgOn = eval(imgName + "_1.src");
        document[imgName].src = imgOn;
        arrowimgOn = eval(imgName + "_a.src");
        document["arrows"].src = arrowimgOn;
    }
 }
 function mast_inact(imgName)
 {  if (version=="good")
    {   imgOff = eval(imgName + "_0.src");
        document[imgName].src = imgOff;
        document["arrows"].src = "graphics/c/subarrows.gif";
    }
 }
 function showCard()
 {       window.open("mission1.htm", "missionWindow","scrollbars=false,width=300,height=200");
 }
 function over(imgName)
 {
 if (version == "good")
 {
    imgOver = eval(imgName + ".src");
    document[imgName].src = imgOver;
 }
 }
 function out(imgName)
 {
    if (version == "good")
    document[imgName].src = eval(imgName + "out.src");
 }
//end hide script from older browsers -->
