<!-- JavaScript Code © 1996 DesertNet . http://desert.net/ -->
<!-- Author: Wil Gerken . wil@desert.net -->
function imgToggle(imgObjectName, imgObjectState) {
  if (navigator.userAgent.indexOf ("Mozilla/3") >= 0) {    imgObject = new Object();
    imgObject = imgObjectName;
    imgObject.src = imgObjectState;
  }
}
function imgPreLoad(i, img) {
  if (navigator.userAgent.indexOf ("Mozilla/3") >= 0) {    cacheImg[i] = new Image();
    cacheImg[i].src = img;
  }
}
// Declare and Load Images
what = "nscorp/html/images/new-br.gif";
transact = "nscorp/html/images/transact-br.gif";
about = "nscorp/html/images/about-br.gif";
business = "nscorp/html/images/business-br.gif";
invest = "nscorp/html/images/invest-br.gif";
journal = "nscorp/html/images/journal-br.gif";
employ = "nscorp/html/images/employ-br.gif";
site = "nscorp/html/images/search-br.gif";
cacheImg = new Array();
imgPreLoad(0,what);
imgPreLoad(1,transact);
imgPreLoad(2,about);
imgPreLoad(3,business);
imgPreLoad(4,invest);
imgPreLoad(5,journal);
imgPreLoad(6,employ);
imgPreLoad(7,site);
// End Script --&gt;
</script>


